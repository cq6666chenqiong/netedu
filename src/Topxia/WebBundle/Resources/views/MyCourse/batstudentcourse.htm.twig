{% extends 'TopxiaWebBundle:CourseStudentManage:index.html.twig' %}

{% block title %}学员管理 - {{ parent() }}{% endblock %}

{% set submenu = 'student' %}

{% set script_controller = 'course-manage/students' %}

{% block maincontent %}



    <div class="form-group" style="width: 100%">
       <form action="{{ path('course_fetch_bat_student',{courseId:courseId}) }}">
           <div class="form-group">
                层级
               <select id="keshi" name="keshi" style="width: 30%;border: 1px solid #ccc;border-radius: 4px;height: 34px;padding: 6px 12px;">
                   <option value=""> 全选 </option>
                   {% for k in classr %}
                       <option {% if keshi == k.id %} selected {% endif %} value="{{ k.id }}"> {{ k.title }} </option>
                   {% endfor %}
               </select>
               专业类别
               <select id="sskeshi" name="sskeshi" style="width: 30%;border: 1px solid #ccc;border-radius: 4px;height: 34px;padding: 6px 12px;">
                   <option value=""> 全选 </option>
                   <option {% if sskeshi == "静疗" %} selected {% endif %} value="静疗"> 静疗 </option>
                   <option {% if sskeshi == "糖尿病" %} selected {% endif %} value="糖尿病"> 糖尿病 </option>
                   <option {% if sskeshi == "危重症" %} selected {% endif %} value="危重症"> 危重症 </option>
                   <option {% if sskeshi == "跌倒" %} selected {% endif %} value="跌倒"> 跌倒 </option>
                   <option {% if sskeshi == "肿瘤" %} selected {% endif %} value="肿瘤"> 肿瘤 </option>
                   <option {% if sskeshi == "管路" %} selected {% endif %} value="管路"> 管路 </option>
                   <option {% if sskeshi == "健康教育" %} selected {% endif %} value="健康教育"> 健康教育 </option>
