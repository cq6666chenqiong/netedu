                   <option {% if sskeshi == "科室运行文件" %} selected {% endif %} value="科室运行文件"> 科室运行文件 </option>
                   <option {% if sskeshi == "临床药品" %} selected {% endif %} value="临床药品"> 临床药品 </option>
                   <option {% if sskeshi == "院感" %} selected {% endif %} value="院感"> 院感 </option>
                   <option {% if sskeshi == "敏感指标" %} selected {% endif %} value="敏感指标"> 敏感指标 </option>
                   <option {% if sskeshi == "造口" %} selected {% endif %} value="造口"> 造口 </option>
                   <option {% if sskeshi == "护理文书" %} selected {% endif %} value="护理文书"> 护理文书 </option>
                   <option {% if sskeshi == "应急" %} selected {% endif %} value="应急"> 应急 </option>
                   <option {% if sskeshi == "宣传" %} selected {% endif %} value="宣传"> 宣传 </option>
                   <option {% if sskeshi == "坠床" %} selected {% endif %} value="坠床"> 坠床 </option>
               </select>
           </div>
           <div class="form-group">
               病区
               <select id="memberType" name="memberType" style="width: 30%;border: 1px solid #ccc;border-radius: 4px;height: 34px;padding: 6px 12px;">
                   <option value=""> 全选 </option>
                   {% for m in memberTypes %}
                       <option value="{{ m.memberTypeName }}"> {{ m.memberTypeName }}  </option>
                   {% endfor %}
               </select>
               行政职务
               <select id="position" name="position" style="width: 30%;border: 1px solid #ccc;border-radius: 4px;height: 34px;padding: 6px 12px;">
                   <option value=""> 全选 </option>
                   <option {% if position == "高级" %} selected {% endif %} value="高级"> 高级 </option>
                   <option {% if position == "护士" %} selected {% endif %} value="护士"> 护士 </option>
                   <option {% if position == "护士长" %} selected {% endif %} value="护士长"> 护士长 </option>
                   <option {% if position == "教学老师" %} selected {% endif %} value="教学老师"> 教学老师 </option>
